  "description": "An addon for running KDE Connect on Home Assistant.",
  "startup": "services",
  "services": [
    {
      "type": "dbus",
      "name": "kdeconnectd",
      "bus": "session",
      "socket": "/run/dbus/session_bus/kdeconnectd.socket"
    }
  ]
}
